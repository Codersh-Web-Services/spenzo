                    <img class="new_top_logo" src="{{section.settings.collectionsub1 | image_url}}" alt="{{section.settings.collectionsub1.alt}}"> <br>
                    <img  class="product_box" src="{{section.settings.collectionsub1product | image_url}}" alt="{{section.settings.collectionsub1.alt}}">
                    <p class="price">{{section.settings.collectionsub1pricetext}}</p>
                    <div class="button">
                        <a href="{{section.settings.collectionsub1.url }}" class="shopcollection">
                            {{ section.settings.collectionsub1text }}
                        </a>
                    </div>
                </div>
                </div>
                <div class="col-lg-6  text-center ps-lg-4 my-5 my-lg-0">
