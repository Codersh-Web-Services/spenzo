                    <div class="col-lg-6 pe-4 megnetic-accesories-section ">
                        <div class="col-md-10 col-xxl-6 ms-md-auto text-center sub-megnetic-accesories">
                            <p class="magnetic-heading">   {{ section.settings.collectionsub3title }}</p>
                            <p class="price"> 
                             {{ section.settings.collectionsub3text }}
                            </p>
                            <div class="button">
                                <a href="{{ section.settings.collectionsub3url }}" class="shopcollection">
                                    {{ section.settings.collectionsub3text2 }}
                                </a>
                            </div>
                        </div>
                    </div>
    
                    <div class="col-lg-6  text-center ps-lg-5 my-5 my-lg-0">
                        <div class="bundleup-section ">
                            <img class="bundleimage" src="{{section.settings.collection4sublogo | image_url }}">
                            <p class="bundle-heading">
                                {{section.settings.collection4subtext }}
                            </p>
                            <div class="row ranfider-section">
    
                                <div class="col-xxl-8 mx-auto d-flex">

                                    {% for block in section.blocks %}
                                        {% case block.type %}
                                          {% when 'collection_feature' %}
                                            <div class="col-3 m-auto border-right-section"> 
                                                {{ block.settings.image | image_url: width: 2048 | image_tag }}
                                                <p class="bundle-title">MAGNETIC RANGEFINDER</p>
                                            </div>
                                          {% endcase %}
                                      {% endfor %}
                                </div>
                            </div>
                            <img src="{{ section.settings.collection4sub | image_url }}" >
                            <div class="pre_order_price">
                                <span class="old-price"><span class="money">{{ section.settings.collection4sub_oldprice }}</span></span>
                                <span class="new-price"><span class="money">{{ section.settings.collection4sub_newprice }}</span></span>
                            </div>
                            <div class="button">
                                <a href="{{section.settings.collection4url}}" class="shopcollection">
                                    {{section.settings.collection4subshoptext }}
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- shop-collection-section end  -->

    <div class="free_shipping_bg">
      
        {% for block in section.blocks %}  
    {% case block.type %}
  
