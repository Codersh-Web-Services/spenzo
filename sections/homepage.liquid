                    <img class="new_top_logo"  src="{{section.settings.collectionsub2 | image_url }}" alt="series2-laser"> <br>
                    <img class="product_box" src="{{section.settings.collectionsub2product | image_url }}" alt="Series2-camera">
                    <p class="price">{{section.settings.collectionsub2pricetext }}</p>
                    <div class="button">
                        <a href="{{ section.settings.collectionsub2url }}" class="shopcollection">
                            {{ section.settings.collectionsub2text }}
                        </a>
                    </div>
                    </div>
                </div>
            </div>
            </div>
            <div class="row shop-collectiontwo-section my-5">
