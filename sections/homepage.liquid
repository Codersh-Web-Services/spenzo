

    <!-- hero-section start here  -->
    <section class="hero-section">
        <div class=" container-fluid">
            <div class="row">
                <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <div class=" d-md-flex">
                                <div class="col-md-5 text-center my-auto">
                                    <img src="{{section.settings.slide1 | image_url }}">
                                    <button onclick="window.location.pathname = '{{section.settings.slide1btnurl}}'" class="shop-btn ">{{section.settings.slide1btn}}</button>
                                </div>
                                <div class="col-md-7">
                                    <img src="{{section.settings.slide1_bg | image_url}}">
                                </div>
                            </div>
                        </div>
                        <div class="carousel-item ">
                            <div class=" d-md-flex">
                                
                                <div class="col-12">
                                    <img  loading="lazy" src="{{section.settings.slide2_bg | image_url }}" alt="section.settings.slide2_bg.alt">
                                </div>
                            </div>
                        </div>
                        <div class="carousel-item ">
                            <div class=" d-md-flex">
                                <div class="col-md-5 text-center my-auto">
                                    <img  loading="lazy" src="{{section.settings.slide1 | image_url }}" alt="{{section.settings.slide1.alt }}">
                                    <button onclick="window.location.pathname = '{{section.settings.slide1btnurl}}'" class="shop-btn ">{{section.settings.slide1btn}}</button>
                                </div>
                                <div class="col-md-7">
                                    <img  loading="lazy" src="{{section.settings.slide1_bg | image_url }}" alt="{{section.settings.slide1_bg.alt }}">
                                </div>
                            </div>
                        </div>
                        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls"
                            data-bs-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="visually-hidden">Previous</span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls"
                            data-bs-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="visually-hidden">Next</span>
                        </button>
                    </div>
                </div>
            </div>
    </section>
    <!-- hero-section end here  -->

    <!-- play-diffrent-section start  -->
    <section class="play-diffrent-section my-5">
        <div class="container-fluid py-5">
            <div class="row">
                <div class="col-md-6 mx-auto text-center">
                    <img src="{{section.settings.logo1 | image_url : width : 100}}">
                    <h3 class="change_way_title ">
                        <p>{{section.settings.sheading1}}</p>
                    </h3>
                    <p class="golf-editor">{{section.settings.sheading2}}</p>
                </div>
            </div>
        </div>
    </section>
    <!-- play-diffrent-section end  -->

    <!-- rated-slope-section start  -->
    <section class="rated-slope-section my-5">
        <div class="container-fluid py-5">
            <div class="row">
                <div class="col-11 mx-auto subrated-slope-section d-lg-flex ">
                    <div class="col-lg-6 mx-auto d-none d-lg-flex mt-auto text-center ">
                        <img src="{{section.settings.product_group | image_url }}" alt="{{section.settings.product_group.alt }}">
                    </div>
                    <div class="col-lg-6 m-auto text-center py-5">
                        <img class="headline-logo"  src="{{section.settings.product_group_logo | image_url }}" alt="{{section.settings.product_group_logo.alt }}">
                        <p class="sub_title">{{section.settings.subheadline}}</p>
                        <div class="row top-icon-section">
                            {% for block in section.blocks %}
                             {% case block.type %}
  
      {% when 'productfeatures' %}  
                                    <div class="col-3 rated-shop-icon">
                                        <div class="top-icon">
                                            {{ block.settings.image | image_url: width: 54 | image_tag }}
                                        </div>
                                        <p>{{ block.settings.text }}</p>
                                    </div>
                                    {% endcase %}
                              {% endfor %}

                          
                         
                        </div>
                        <div class="price my-5">
                            <span class="money">{{section.settings.pricetext}}</span>
                        </div>
                        <div class="button mt-5">
                            <a href="{{section.settings.maincollectionbtn_url}}" class="shopcollection">
                                {{section.settings.maincollectionbtn}}
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-6 mx-auto d-flex d-lg-none mt-auto text-center ">
                        <img src="{{section.settings.product_group | image_url }}" alt="{{section.settings.product_group.alt }}">
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- rated-slope-section end  -->

    <!-- shop-collection-section start -->
    <section class="shop-collection-section">
        <div class="container-fluid">
            <div class="row ">
                <div class="col-11 mx-auto d-lg-flex ">
                <div class="col-lg-6  text-center pe-4">
